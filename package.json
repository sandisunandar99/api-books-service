{
  "name": "api-books-service",
  "version": "1.0.0",
  "description": "Api Book Service",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc --noEmitOnError false",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit --verbose",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles --verbose",
    "migrate": "typeorm-ts-node-esm migration:run -d src/database/data-source.ts",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d src/database/data-source.ts",
    "schema:sync": "typeorm-ts-node-esm schema:sync -d src/database/data-source.ts"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "mariadb": "^3.4.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandisunandar99/api-books-service.git"
  },
  "author": "sandi sunandar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sandisunandar99/api-books-service/issues"
  },
  "homepage": "https://github.com/sandisunandar99/api-books-service#readme"
}
